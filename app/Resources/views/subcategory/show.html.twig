{% extends 'base.html.twig' %}

{% block body %}
    <h1>Subcategory</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subcategory.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ subcategory.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ subcategory.description }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ subcategory.status }}</td>
            </tr>
            <tr>
                <th>Createddatetime</th>
                <td>{% if subcategory.createdDateTime %}{{ subcategory.createdDateTime|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Categoryid</th>
                <td>{{ subcategory.categoryId }}</td>
            </tr>
            <tr>
                <th>Createdby</th>
                <td>{{ subcategory.createdBy }}</td>
            </tr>
            <tr>
                <th>Updateddatetime</th>
                <td>{% if subcategory.updatedDateTime %}{{ subcategory.updatedDateTime|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedby</th>
                <td>{{ subcategory.updatedBy }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('subcategory_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('subcategory_edit', { 'id': subcategory.id }) }}">Edit</a>
        </li>
    </ul>
{% endblock %}
